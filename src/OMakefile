BYTE_ENABLED = true
NATIVE_ENABLED = true
USE_OCAMLFIND = true

.PHONY: clean
.SUBDIRS: abstract

PROGRAM = test

OCAML_LIBS[] =
	abstract/libabstract

OCAMLINCLUDES += $(dir abstract)

OCAML_OTHER_LIBS[] =
	unix
	str

FILES[] =
	telnet

.DEFAULT: $(OCamlProgram $(PROGRAM), $(FILES))

clean:
	rm *.c* *.o* $(PROGRAM)*
